<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mozi [
    <!ELEMENT mozi (film*)>
    <!ELEMENT film (rendezo+, szinesz+, iro+)>
    <!ATTLIST film
        id ID #REQUIRED
        mufaj (horror|akcio|vigjatek) #REQUIRED>
    <!ELEMENT rendezo EMPTY>
    <!ATTLIST rendezo
        id ID #REQUIRED
        nev CDATA #IMPLIED>
    <!ELEMENT szinesz EMPTY>
    <!ATTLIST szinesz
        id ID #REQUIRED
        nev CDATA #IMPLIED>
    <!ELEMENT iro EMPTY>
    <!ATTLIST iro id ID #REQUIRED>
]>
<mozi>
    <film id="txn_nxm_2jb" mufaj="horror">
        <rendezo id="nzk_4xm_2jb" nev="alex" />
        <szinesz id="u5g_4xm_2jb" nev="Madison_Ivy" />
        <szinesz id="a" />
        <iro id="trv_4xm_2jb" />
    </film>
</mozi>